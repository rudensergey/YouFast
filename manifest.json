{
   "name": "YouFast",
   "version": "0.01",
   "description": "Google Chrome extention which helps to increase a video rate more than 2x times",

   "background": {
      "scripts": ["background.js"],
      "persistent": false
   },

   "page_action": {
      "default_popup": "popup.html"
   },

   "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
   },

   "icons": {
      "128": "images/128.png",
      "48": "images/48.png",
      "32": "images/32.png",
      "16": "images/16.png"
   },

   "manifest_version": 2
}
